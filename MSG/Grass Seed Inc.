#include <bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    double c,n,w,l,y=0;
    cin >> c >> n;
    while(cin >> l)
    {
        cin >> w;
        y+=(l*w)*c;
    }
    cout << fixed << setprecision(8) << y;
    return 0;
}
