#include <bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    int n;
    string s;
    cin >> n;
    for(int i=0;i<n;++i)
    {
        cin >> s;
        cout <<"Takk " << s << endl;
    }
    return 0;
}
