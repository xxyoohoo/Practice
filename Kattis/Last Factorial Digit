#include <bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    int n=1,x,y;
    cin >> y;
    for(int i=0;i<y;++i)
    {
        cin >> x;
        for(int j=1;j<=x;++j)
        {
            n*=j;
            n%=10;
        }
        cout << n << endl;
        n=1;
    }
    return 0;
}
