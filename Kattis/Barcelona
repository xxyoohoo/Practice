#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n,x,y;
    cin >> n >> x;
    for(int i=1;i<=n;++i)
    {
        cin >> y;
        if(y==x)
        {
            if(i==1) cout << "fyrst";
            else if(i==2) cout << "naestfyrst";
            else cout << i << " fyrst";
            return 0;
        }
    }
}
