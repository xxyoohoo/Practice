#include <bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    int n,x,y=0,z=0;
    cin >> n;
    for(int i=0;i<n;++i)
    {
        cin >> x;
        y+=x;
    }
    for(int i=0;i<n-1;++i)
    {
        cin >> x;
        z+=x;
    }
    cout << y-z;
    return 0;
}
